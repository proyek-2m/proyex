{
	"name": "proyex",
	"version": "1.0.0",
	"private": true,
	"type": "module",
	"scripts": {
		"build": "npm run generate:importmap && npm run generate:types && npm run mantine::generate && eslint . && cross-env NODE_OPTIONS=--no-deprecation next build --webpack",
		"dev": "npm run generate:importmap && npm run mantine::generate && cross-env NODE_OPTIONS=--no-deprecation next dev",
		"devsafe": "rm -rf .next && npm run generate:importmap && npm run generate:types && cross-env NODE_OPTIONS=--no-deprecation next dev",
		"generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
		"generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
		"mantine::generate": "npx tailwind-preset-mantine ./src/lib/modules/theme-mantine.ts -o ./src/styles/mantine.css",
		"lint": "eslint .",
		"payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
		"start": "cross-env NODE_OPTIONS=--no-deprecation next start"
	},
	"dependencies": {
		"@mantine/carousel": "^8.3.10",
